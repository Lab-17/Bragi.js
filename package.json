{
  "name": "@lab17/bragi",
  "version": "0.0.0-development",
  "description": "A audio library for the web.",
  "license": "MIT",
  "author": {
    "name": "Vin√≠cius Carvalho",
    "email": "vinicius@criptext.com",
    "url": "https://github.com/Oak-V"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/lab-17/bragi.git"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "bragi",
    "bragi.js",
    "audio",
    "sound",
    "stream",
    "web audio",
    "webaudio",
    "browser",
    "html5",
    "html5 audio",
    "audio sprite",
    "audiosprite",
    "audio stream",
    "audiostream",
    "web socket",
    "websocket"
  ],
  "scripts": {
    "build": "rollup -c",
    "docs": "typedoc src/index.ts",
    "release": "semantic-release",
    "test": "run-s 'test:*'",
    "test:clear": "rimraf .nyc_output coverage",
    "test:run": "run-p 'test:run:*'",
    "test:run:server": "jest",
    "test:run:client": "cypress run",
    "clear": "rimraf lib docs coverage .nyc_output",
    "reset": "yarn clear node_modules",
    "coverage": "run-s test 'coverage:*'",
    "coverage:copy": "run-p 'coverage:copy:*'",
    "coverage:copy:client": "cpy 'coverage/client/*.json' coverage --rename=coverage-client.json",
    "coverage:copy:server": "cpy 'coverage/server/*.json' coverage --rename=coverage-server.json",
    "coverage:merge": "nyc merge coverage .nyc_output/out.json",
    "coverage:_report": "nyc report --reporter lcov --reporter text --report-dir coverage",
    "coverage:clear": "rimraf coverage/client coverage/server",
    "lint": "run-s 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "fix": "run-s 'fix:*'",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint --fix .",
    "upgrade": "run-s 'upgrade:*'",
    "upgrade:yarn": "yarn set version latest",
    "upgrade:deps": "yarn upgrade-interactive",
    "upgrade:test": "yarn test",
    "upgrade:docs": "yarn docs",
    "upgrade:build": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/code-coverage": "^3.8.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/cypress": "^7.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/browserify": "^12.0.36",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.10.3",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-plugin-istanbul": "^6.0.0",
    "commitizen": "^4.2.1",
    "cpy-cli": "^3.1.1",
    "cypress": "^5.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.1",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.1.2",
    "standardized-audio-context": "^25.0.2",
    "standardized-audio-context-mock": "^8.4.0",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  },
  "optionalDependencies": {
    "standardized-audio-context": "^25.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
